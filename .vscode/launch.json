{
  "configurations": [
    {
        "name": "调试main",
        "type": "cppdbg",
        "request": "launch", 
        "program": "${workspaceRoot}/.temp/main.out",
        "stopAtEntry": false,
        "cwd": "${workspaceFolder}/.temp/",
        "externalConsole": false,
        "internalConsoleOptions": "neverOpen",
        "MIMode": "gdb",
        "setupCommands": [
            {
                "description": "为 gdb 启用整齐打印",
                "text": "-enable-pretty-printing",
                "ignoreFailures": true
            },
            {
                "description":  "将反汇编风格设置为 Intel",
                "text": "-gdb-set disassembly-flavor intel",
                "ignoreFailures": true
            }
        ],
        "preLaunchTask": "编译测试文件",
    },
        {
            "name": "调试特定test",
            "type": "cppdbg",
            "request": "launch", 
            "program": "${workspaceRoot}/build_arch/tests/send_connect",
            "stopAtEntry": false,
            "externalConsole": false,
            "internalConsoleOptions": "neverOpen",
            "MIMode": "gdb",
            "cwd": "${workspaceFolder}/build_arch/",
            "setupCommands": [
                {
                    "description": "为 gdb 启用整齐打印",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                },
                {
                    "description":  "将反汇编风格设置为 Intel",
                    "text": "-gdb-set disassembly-flavor intel",
                    "ignoreFailures": true
                }
            ],
    },
    {
        "name": "调试webget",
        "type": "cppdbg",
        "request": "launch", 
        "program": "${workspaceRoot}/build/apps/webget",
        "args": [
            "cs144.keithw.org", 
            "hello"
        ],
        "stopAtEntry": false,
        "cwd": "${workspaceFolder}/libsponge/util",
        "externalConsole": false,
        "internalConsoleOptions": "neverOpen",
        "MIMode": "gdb",
        "setupCommands": [
            {
                "description": "为 gdb 启用整齐打印",
                "text": "-enable-pretty-printing",
                "ignoreFailures": true
            },
            {
                "description":  "将反汇编风格设置为 Intel",
                "text": "-gdb-set disassembly-flavor intel",
                "ignoreFailures": true
            }
        ]
    },

    {
      "name": "Python: 当前文件",
      "type": "python",
      "request": "launch",
      "program": "${file}",
      "console": "integratedTerminal"
    },
    
  ]
}
